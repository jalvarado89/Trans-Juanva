<%= form_for(route) do |f| %>
  <% if route.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(route.errors.count, "error") %> prohibited this route from being saved:</h2>

      <ul>
      <% route.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :Id %>
    <%= f.text_field :Id %>
  </div>

  <div class="field">
    <%= f.label :Nombre %>
    <%= f.text_field :Nombre %>
  </div>

  <div class="field">
    <%= f.label :Precio_Empresa %>
    <%= f.number_field :Precio_Empresa %>
  </div>

  <div class="field">
    <%= f.label :Precio_Chofer %>
    <%= f.number_field :Precio_Chofer %>
  </div>

  <div class="field">
    <%= f.label :Id_Empresa %>
    <%= f.number_field :Id_Empresa %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
